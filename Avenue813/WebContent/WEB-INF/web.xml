<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Avenue813</display-name>
  <welcome-file-list>
    <welcome-file>../PaginaHome/homepage.html</welcome-file>
  </welcome-file-list>
  
  <resource-ref>
  	<description>JNDI reference to a data source</description>
  	<res-ref-name>jdbc/database</res-ref-name>
  	<res-type>javax.sql.DataSource</res-type>
  	<res-auth>Container</res-auth>
  </resource-ref>
  
  <filter>
  	<filter-name>RegisterFilter</filter-name>
  	<filter-class>it.avenue813.filters.RegisterFilter</filter-class>
  </filter>
  
   <filter-mapping>
    <filter-name>RegisterFilter</filter-name>
    <url-pattern>/PaginaAutenticazione/register.jsp</url-pattern>
  </filter-mapping>
  
</web-app>